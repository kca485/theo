---
style: story-gallery
---
{% extends "base.njk" %}
{% block layout %}
  <h2>{{title}}</h2>
  <ol>
  {% for story in collections[stories] %}
    <li>
      <a class="story" href="{{ story.url }}">
        <p class="story-title">{{ story.data.title }}</p>
      </a>
    </li>
  {% endfor %}
  </ol>
  {{ content | safe }}
{% endblock %}
